{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "BankDocsExtraction",
  "type": "object",
  "required": ["docType", "meta", "fields"],
  "properties": {
    "docType": {"enum": ["receipt", "contract", "statement"]},
    "meta": {
    "type": "object",
    "properties": {
    "pages": {"type": "integer"},
    "lang": {"type": "string"},
    "confidence": {"type": "number"}
    }
    },
    "fields": {"type": "object"},
    "lineItems": {"type": "array", "items": {"type": "object"}}
  }
}